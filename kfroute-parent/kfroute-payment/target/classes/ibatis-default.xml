	<sqlMap resource="sqlmap/paymentAccount.xml"/>
	<sqlMap resource="sqlmap/shortMsg.xml"/>
	<sqlMap resource="sqlmap/merchant.xml"/>
	<sqlMap resource="sqlmap/latn.xml"/>
	
